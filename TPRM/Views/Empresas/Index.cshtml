@model IEnumerable<TPRM.Models.Empresa>

@{
    ViewBag.Title = "Index";
}

<h2>Empresas</h2>

<p>
    @Html.ActionLink("Cadastrar Empresa", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CNPJ)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RazaoSocial)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CNPJ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RazaoSocial)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EmpresaID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EmpresaID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EmpresaID })
        </td>
    </tr>
}

</table>
