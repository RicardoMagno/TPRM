@model IEnumerable<TPRM.Models.CustomDebitoCliente>

@{
    ViewBag.Title = "Listagem de Gastos por Clientes";
}

<h2>Listagem de Gastos por Clientes</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ValorDebito)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorDebito)
            </td>
        </tr>
    }

</table>
